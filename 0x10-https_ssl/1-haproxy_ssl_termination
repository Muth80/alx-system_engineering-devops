frontend www-https
    bind *:443 ssl crt /etc/letsencrypt/live/www.holberton.online/fullchain.pem
    reqadd X-Forwarded-Proto:\ https
    default_backend www-backend

backend www-backend
    mode http
    balance roundrobin
    server webserver1 10.0.0.1:80 check
    server webserver2 10.0.0.2:80 check
